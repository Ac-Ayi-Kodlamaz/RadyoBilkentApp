<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="podcasts1RowModel" type="com.application.app.modules.podcasts.data.model.Podcasts1RowModel"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:clickable="true" android:id="@+id/constraintGroup"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup185"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" style="@style/groupStyleblue_400">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup184"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="0dp"
                app:layout_constraintBottom_toTopOf="@+id/imageImage3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent">
                <TextView android:id="@+id/txtPodcastEditor"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_57pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content"
                    android:text="@{podcasts1RowModel.txtPodcastEditor}"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="2.2"
                    app:layout_constraintStart_toEndOf="@+id/constraintRectangle29"
                    app:layout_constraintTop_toTopOf="@+id/txtPhotoTitle"
                    style="@style/txtRobotoregular12" tools:text="@string/msg_podcast_editor"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintRectangle29"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_57pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_width="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtPodcastEditor"
                    app:layout_constraintHorizontal_bias="0.61"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" style="@style/groupStylegray_600">
                    <ImageView android:id="@+id/imageMaskgroup"
                        android:layout_height="0dp"
                        android:layout_width="@dimen/_60pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_maskgroup_9"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintDimensionRatio="1.0:0.96666664"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <TextView android:gravity="start"
                    android:id="@+id/txtPhotoTitle"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="0dp"
                    android:text="@{podcasts1RowModel.txtPhotoTitle}"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/constraintRectangle29"
                    app:layout_constraintStart_toStartOf="@+id/constraintRectangle29"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/txtRobotoregular12" tools:text="@string/msg_photo_title2"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <ImageView android:id="@+id/imageImage3"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_161pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_image3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="0.9375:1.0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/constraintGroup184"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <ImageView android:id="@+id/imageImage2"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_141pxh"
            android:layout_marginTop="@dimen/_139pxv"
            android:layout_width="@dimen/_21pxh"
            android:scaleType="fitXY" android:src="@drawable/img_image2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="0.5833333:1.0"
            app:layout_constraintEnd_toEndOf="@+id/constraintGroup185"
            app:layout_constraintStart_toStartOf="@+id/constraintGroup185" app:layout_constraintTop_toTopOf="@+id/constraintGroup185"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
